{
  "name": "my-electron-app",
  "version": "0.1.0",
  "author": "wlcks311",
  "description": "My Electron app",
  "main": "main.js",
  "homepage": ".",
  "dependencies": {
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "socket.io": "^4.7.1"
  },
  "scripts": {
    "start": "electron .",
    "deploy": "electron-builder --windows nsis:ia32"
  },
  "build": {
    "productName": "test",
    "appId": "com.electron.hello",
    "asar": true,
    "protocols": {
      "name": "test",
      "schemes": [
        "test"
      ]
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./resources/installer/Icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "./resources/installer/",
      "output": "./exeResult/",
      "app": "."
    }
  },
  "devDependencies": {
    "electron-builder": "^24.6.4",
    "electron": "^8.1.1"
  }
}
